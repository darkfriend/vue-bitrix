{
  "name": "vue-bitrix",
  "version": "0.0.5",
  "description": "A Vue components for Bitrix CMS",
  "main": "./dist/vue-bitrix.common.js",
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "build-lib": "vue-cli-service build --target lib --name vue-bitrix src/index.js",
    "lint": "vue-cli-service lint"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/darkfriend/vue-bitrix.git"
  },
  "keywords": [
    "bitrix",
    "darkfriend",
    "bitrix components",
    "components"
  ],
  "author": "darkfriend",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/darkfriend/vue-bitrix/issues"
  },
  "homepage": "https://github.com/darkfriend/vue-bitrix#readme",
  "dependencies": {
    "core-js": "^3.6.5",
    "vue": "^2.6.11",
    "vue-dev-mixins": "^1.0.2"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "~4.5.0",
    "@vue/cli-plugin-eslint": "~4.5.0",
    "@vue/cli-service": "~4.5.0",
    "babel-eslint": "^10.1.0",
    "eslint": "^6.7.2",
    "eslint-plugin-vue": "^6.2.2",
    "vue-template-compiler": "^2.6.11"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "parserOptions": {
      "parser": "babel-eslint"
    },
    "rules": {}
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ],
  "files": [
    "dist/*",
    "src/*",
    "public/*",
    "*.js",
    "*.json"
  ]
}
